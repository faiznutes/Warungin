{
  "name": "warungin",
  "version": "1.1.0",
  "description": "Multi-Tenant System for UMKM",
  "main": "dist/src/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/src/app.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:safe": "node scripts/migrate-database.js",
    "prisma:migrate:quick": "node scripts/quick-migrate.js",
    "prisma:migrate:manual": "node scripts/apply-manual-migration.js",
    "prisma:migrate:payment-mapping": "node scripts/apply-payment-mapping-migration-v2.js",
    "prisma:push": "prisma db push --accept-data-loss",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "seed:demo": "ts-node prisma/seed-demo.ts",
    "create:super-admin": "ts-node scripts/create-super-admin.ts",
    "create:super-admin:docker": "node scripts/create-super-admin-docker.js",
    "push:schema:supabase": "node scripts/push-schema-supabase.js",
    "setup:super-admin:supabase": "node scripts/setup-super-admin-supabase.js",
    "check:db": "node scripts/check-db.js",
    "test:login": "node scripts/test-login.js",
    "list:users": "node scripts/list-users.js",
    "verify:passwords": "node scripts/verify-passwords.js",
    "reset:password": "node scripts/reset-password.js",
    "create:tenant-user": "node scripts/create-tenant-user.js",
    "create:demo-user": "node scripts/create-demo-tenant-user.js",
    "fix:super-admin": "node scripts/fix-super-admin.js",
    "test:all-logins": "node scripts/test-all-logins.js",
    "reset:all-passwords": "node scripts/reset-all-passwords.js",
    "prisma:reset": "prisma migrate reset",
    "migrate:supabase": "node scripts/migrate-to-supabase.js",
    "migrate:latest:supabase": "node scripts/migrate-latest-to-supabase.js",
    "migrate:schema:supabase": "node scripts/migrate-latest-to-supabase.js --schema-only",
    "fix:connection": "node scripts/auto-fix-connection.js",
    "update:order-numbers": "node scripts/update-order-numbers.js",
    "test:connection": "node scripts/test-db-connection.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:addons": "node scripts/test-addon-api.js",
    "test:addons:guide": "echo 'See docs/QUICK_ADDON_TEST.md for testing guide'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "check": "node scripts/check-compatibility.js",
    "setup": "node scripts/check-compatibility.js && npm install && cd client && npm install && cd .."
  },
  "keywords": [
    "warungin",
    "multi-tenant",
    "umkm",
    "business-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/pdfkit": "^0.17.3",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "nodemailer": "^6.9.7",
    "otplib": "^12.0.1",
    "pdfkit": "^0.17.2",
    "pdfmake": "^0.2.20",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/csrf": "^1.3.2",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfmake": "^0.2.12",
    "@types/qrcode": "^1.5.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^6.19.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
